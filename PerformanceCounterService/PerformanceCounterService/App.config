<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="PerformanceCounterService" type="PerformanceCounterService.Configuration.PerformanceCounterServiceSection, PerformanceCounterService"/>
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
  <appSettings>
    <add key="NancyHost" value="http://localhost:9527" />
  </appSettings>
  <PerformanceCounterService>
    <PerformanceCounter categoryName="ASP.NET v4.0.30319" counterName="Requests Current"></PerformanceCounter>
    <PerformanceCounter categoryName="ASP.NET v4.0.30319" counterName="Requests Queued"></PerformanceCounter>
    <PerformanceCounter categoryName="Processor" counterName="% Processor Time" instanceName="_Total"></PerformanceCounter>
    <PerformanceCounter categoryName="Memory" counterName="% Committed Bytes In Use"></PerformanceCounter>
  </PerformanceCounterService>
</configuration>